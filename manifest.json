{
    "name": "HMK DOM Customizer",
    "version": "1.0.0",
    "description": "making standups easier est. 2022",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
      ],
    "action": {
        "default_popup": "index.html",
        "default_title": "Hallmark DOM Customizer Settings"
    },
    "content_scripts": [
        {
          "js": [
            "script.js"
          ],
          "matches": [
            "https://hallmark.atlassian.net/jira/software/c/projects/DOM/boards/*"
          ]
        }
      ]
}